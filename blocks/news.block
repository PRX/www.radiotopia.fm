{.repeated section items}
  <section class="news">
    <article class="news-item">
      <div class="news-content">
        <p class="news-date">{addedOn}</p>
        <p class="news-title">{title}</p>

        <div class="news-excerpt">{excerpt}</div>
      </div>

      <img class="news-image" src={assetUrl}/>
    </article>
  </section>
{.end}
