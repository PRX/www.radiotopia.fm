            <li><input type="checkbox" value="1" name="group[9413][1]" id="mce-group[9413]-9413-0"><label for="mce-group[9413]-9413-0">99% Invisible</label></li>
            <li><input type="checkbox" value="2" name="group[9413][2]" id="mce-group[9413]-9413-1"><label for="mce-group[9413]-9413-1">The Allusionist</label></li>
            <li><input type="checkbox" value="4" name="group[9413][4]" id="mce-group[9413]-9413-2"><label for="mce-group[9413]-9413-2">The Bugle</label></li>
            <li><input type="checkbox" value="8" name="group[9413][8]" id="mce-group[9413]-9413-3"><label for="mce-group[9413]-9413-3">Criminal</label></li>
            <li><input type="checkbox" value="16" name="group[9413][16]" id="mce-group[9413]-9413-4"><label for="mce-group[9413]-9413-4">Fugitive Waves</label></li>
            <li><input type="checkbox" value="32" name="group[9413][32]" id="mce-group[9413]-9413-5"><label for="mce-group[9413]-9413-5">The Heart</label></li>
            <li><input type="checkbox" value="64" name="group[9413][64]" id="mce-group[9413]-9413-6"><label for="mce-group[9413]-9413-6">Love + Radio</label></li>
            <li><input type="checkbox" value="128" name="group[9413][128]" id="mce-group[9413]-9413-7"><label for="mce-group[9413]-9413-7">The Memory Palace</label></li>
            <li><input type="checkbox" value="256" name="group[9413][256]" id="mce-group[9413]-9413-8"><label for="mce-group[9413]-9413-8">Millennial</label></li>
            <li><input type="checkbox" value="512" name="group[9413][512]" id="mce-group[9413]-9413-9"><label for="mce-group[9413]-9413-9">Mortified</label></li>
            <li><input type="checkbox" value="1024" name="group[9413][1024]" id="mce-group[9413]-9413-10"><label for="mce-group[9413]-9413-10">Radio Diaries</label></li>
            <li><input type="checkbox" value="2048" name="group[9413][2048]" id="mce-group[9413]-9413-11"><label for="mce-group[9413]-9413-11">Song Exploder</label></li>
            <li><input type="checkbox" value="4096" name="group[9413][4096]" id="mce-group[9413]-9413-12"><label for="mce-group[9413]-9413-12">Strangers</label></li>
            <li><input type="checkbox" value="8192" name="group[9413][8192]" id="mce-group[9413]-9413-13"><label for="mce-group[9413]-9413-13">Benjamen Walker's Theory of Everything</label></li>
            <li><input type="checkbox" value="16384" name="group[9413][16384]" id="mce-group[9413]-9413-14"><label for="mce-group[9413]-9413-14">The Truth</label></li>
            <li><input type="checkbox" value="32768" name="group[9413][32768]" id="mce-group[9413]-9413-15"><label for="mce-group[9413]-9413-15">The West Wing Weekly</label></li>
          </ul>
        </div>
          <div id="mce-responses" class="clear">
            <div class="response" id="mce-error-response" style="display:none"></div>
            <div class="response" id="mce-success-response" style="display:none"></div>
          </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_b030d898f636b90f47f8cd820_76ff76f2c5" tabindex="-1" value=""></div>
            <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
          </div>
        </form>
      </div>
      <script type='text/javascript' src='//s3.amazonaws.com/downloads.mailchimp.com/js/mc-validate.js'></script><script type='text/javascript'>(function($) {window.fnames = new Array(); window.ftypes = new Array();fnames[0]='EMAIL';ftypes[0]='email';fnames[1]='FNAME';ftypes[1]='text';fnames[2]='LNAME';ftypes[2]='text';fnames[3]='MMERGE3';ftypes[3]='zip';}(jQuery));var $mcj = jQuery.noConflict(true);</script>
      <!--End mc_embed_signup-->


      <footer id="footer" role="contentinfo">
        <div class="footer-inner">
          <div class="nav-wrapper back-to-top-nav"><nav><div class="back-to-top"><a href="#header">Back to Top</a></div></nav></div>
          <squarespace:block-field id="footerBlocks" columns="12" label="Footer Content" />
          <squarespace:navigation navigationId="footerNav" template="footer-navigation" />
          {.if website.location.addressTitle || website.location.addressLine1 || website.location.addressLine2 || website.location.addressCountry}<div id="siteInfo">{.or}{.if websiteSettings.contactPhoneNumber || websiteSettings.contactEmail}<div id="siteInfo">{.end}{.end}{.section website}{.section location}{.if addressTitle || addressLine1 || addressLine2 || addressCountry}<span class="site-address">{.end}{.if addressTitle}{addressTitle}{.end}{.if addressLine1}{.if addressTitle}, {.end}{addressLine1}{.end}{.if addressLine2}{.if addressTitle || addressLine1}, {.or}{.end}{addressLine2}{.end}{.if addressCountry}{.if addressTitle || addressLine1 || addressLine2}, {.end}{addressCountry}{.end}{.if addressTitle || addressLine1 || addressLine2 || addressCountry}</span>{.end}{.end}{.end}{.section websiteSettings}{.if contactPhoneNumber}<span rel="tel" class="site-phone">{contactPhoneNumber}</span>{.end}{.if contactEmail}<a href="mailto:{contactEmail}" class="site-email"><span>{contactEmail}</span></a>{.end}{.end}{.if website.location.addressTitle || website.location.addressLine1 || website.location.addressLine2 || website.location.addressCountry}</div>{.or}{.if websiteSettings.contactPhoneNumber || websiteSettings.contactEmail}</div>{.end}{.end}
        </div>
      </footer>
    </div>

    <squarespace:script src="site-bundle.js" combo="false" />
    {squarespace-footers}


    <script
    src="https://code.jquery.com/jquery-1.12.4.min.js"
    integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ="
    crossorigin="anonymous"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>

    <squarespace:script src="vendor/underscore-min.js" combo="false" />
    <squarespace:script src="podcast.js" combo="false" />

    <script type="text/javascript">
      $(document).ready(function(){
        RadiotopiaPodcast.initialize();
      });
    </script>
  </body>
</html>
