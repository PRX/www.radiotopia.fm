<section class="events">
  {.repeated section items}
      <article class="event-item">
        <div class="event-content">
          <p class="event-header">Event</p>
          <p class="event-date">{startDate|date %A, %B, %d}</p>
          <p class="event-title">{title}</p>
          <p class="event-location">{location}</p>

          <div class="event-excerpt">{excerpt}</div>
        </div>

        <img class="event-image" src={assetUrl}/>
      </article>
  {.end}
</section>
