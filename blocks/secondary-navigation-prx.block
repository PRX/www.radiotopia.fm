{.section items}
  {.repeated section @}
    {.folder?}
      <ul>
        <li><b>{collection.navigationTitle}</b></li>

        {.repeated section items}
          {.section externalLink}
            <li><a href="{url}">{title}</a></li>
          {.end}
        {.end}
      </ul>
    {.end}
  {.end}
{.end}
